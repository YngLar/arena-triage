@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}


<div class="row">
        <div class="col-md-6">
            <h2>Triage Overview</h2>
            <table class="table table-striped table-bordered">
                <tr>
                    <th>Name</th>
                    <th>Triage Level</th>
                    <th>Assigned Doctor</th>
                    
                </tr>

                @foreach(var item in Model.Patients)
                {   
                    <tr>
                        <td>@item.Name</td>
                        <td>@item.Triage</td>
                        <td>@item.Doctor</td>
                    </tr>
                }
            </table>
        </div>
 </div>
